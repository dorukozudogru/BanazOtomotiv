                            <a href="~/Content/Site/assets/imgs/all/AKARYAKIT01.jpeg" class="portfolio-image popup-gallery">
                                <img src="~/Content/Site/assets/imgs/all/AKARYAKIT01.jpeg" alt="Banaz Otomotiv Akaryakıt İstasyonu" />
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <div id="contact" class="bg-lighter u-PaddingTop150 u-xs-PaddingTop30 u-PaddingBottom150 u-xs-PaddingBottom30">
        <div class="container">
            <div class="row text-center">
                <div class="col-md-8 col-md-offset-2 col-sm-10 col-sm-offset-1 text-center">
                    <h1 class="u-MarginTop10 u-MarginBottom35">Bize Ulaşın</h1>
                </div>
            </div>
            <div class="row">
                <form class="js-ContactForm" method="post">
                    <div class="col-md-6">
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="İsim Soyisim" name="fullname" data-error="İsim alanı zorunludur" required>
                            <div class="help-block with-errors"></div>
                        </div>
                        <div class="form-group">
                            <input type="email" class="form-control" placeholder="E-Posta" name="email" data-error="E-Posta alanı zorunludur" required>
                            <div class="help-block with-errors"></div>
                        </div>
                        <div class="form-group">
                            <input type="tel" class="form-control" placeholder="Telefon" name="phone">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <textarea class="form-control" rows="4" placeholder="Mesajınız" name="message" data-error="Mesaj alanı zorunludur" required></textarea>
                            <div class="help-block with-errors"></div>
                        </div>
                        <button type="submit" class="btn btn-gradient btn--alien btn-block sendButton">Gönder</button>
                    </div>
                </form>
                <div class="col-md-12">
                    <div class="panel panel-body u-MarginTop100 u-xs-MarginTop30">
                        <div id="map" class="map-area"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
